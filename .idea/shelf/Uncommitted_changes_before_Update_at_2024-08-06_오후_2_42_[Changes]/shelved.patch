+import pandas as pd
+import numpy as np
+
+path = 'https://raw.githubusercontent.com/umyunsang/MLSummerBootcamp/master/weather1.csv'
+weather = pd.read_csv(path, index_col=0)
+print(weather.head())
+print(weather.tail())
+print(weather.shape)
+
+# 판다스를 이용한 데이터 분석
+print(weather.describe())
+print(weather.mean())
+print(weather.std())
+
+# 데이터 정제와 결손값의 처리
+print(weather.count())
+missing_data = weather[weather['최대풍속'].isna()]
+print(missing_data)
+weather.fillna(weather['평균풍속'].mean(), inplace=True)
+print(weather.loc['2012-02-12'])
+
+# 시계열 데이터 분석
+d_list = ['01/03/2018','01/03/2018','2018/01/05','2018/01/06']
+print(pd.DatetimeIndex(d_list).year)
+print(pd.DatetimeIndex(d_list).month)
+print(pd.DatetimeIndex(d_list).day)
